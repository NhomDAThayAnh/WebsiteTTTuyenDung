@model IEnumerable<DAWEB.Models.KHACHHANG>

@{
    ViewBag.Title = "QuanLyTaiKhoanNguoiDung";
    Layout = "~/Views/Shared/_layoutQuanLyTaiKhoan.cshtml";
}

<h2 style="text-align:center">Quản lý tài khoản người dùng</h2>

<p>
    @Html.ActionLink("Create New", "ThemTaiKhoanNguoiDung")
</p>
<table class="table">
    <tr>
       
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Repeat_Password)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
     
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Repeat_Password)
        </td>
        <td>
            @Html.ActionLink("Edit", "EditTaiKhoanNguoiDung", new { @IDND = item.IDND }) |
           @Html.ActionLink("Delete", "DeleteTaiKhoanNguoiDung", new { @IDND = item.IDND }) |
            
        </td>
    </tr>
}

</table>
